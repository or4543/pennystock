{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualNetworks_vn_pennystock_dev_westuk_name": {
            "defaultValue": "vn-pennystock-dev-westuk",
            "type": "String"
        },
        "networkSecurityGroups_WebNSG_pennystock_dev_westuk_externalid": {
            "defaultValue": "/subscriptions/2b461bac-b0bd-4867-802e-73a484b83fb4/resourceGroups/rg-pennystock-dev-westuk/providers/Microsoft.Network/networkSecurityGroups/WebNSG-pennystock-dev-westuk",
            "type": "String"
        },
        "networkSecurityGroups_DBNSG_pennystock_dev_westuk_externalid": {
